<VBox fx:id="Start" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.chat_bot.WindowMainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <Button layoutX="26.0" layoutY="116.0" mnemonicParsing="false" onAction="#onConnectButtonClick" prefHeight="35.0" prefWidth="149.0" text="Вход">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <TextField fx:id="TextFieldUserName" layoutX="26.0" layoutY="75.0" text="Dmitry" />
      <Label layoutX="7.0" layoutY="31.0" text="Введите имя пользователя:">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="LabelError" layoutX="86.0" layoutY="169.0" />
   </AnchorPane>
</VBox>
